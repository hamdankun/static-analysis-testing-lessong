{
    "name": "static-analysis-testing",
    "version": "1.0.0",
    "description": "static analysis testing lesson",
    "main": "index.js",
    "scripts": {
        "build": "babel src --out-dir dist",
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "eslint --ignore-path .gitignore .",
